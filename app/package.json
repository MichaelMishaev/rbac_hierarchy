{
  "name": "corporations-mvp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3200",
    "build": "prisma generate && next build",
    "postbuild": "npx prisma db push --accept-data-loss && npx prisma db seed || true",
    "start": "next start -H 0.0.0.0 -p ${PORT:-8080}",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "railway:sync": "bash scripts/railway-db-sync.sh",
    "db:seed:cities": "tsx scripts/seed-israeli-cities.ts",
    "db:seed:neighborhoods": "tsx scripts/seed-neighborhoods.ts",
    "db:check-integrity": "tsx scripts/check-worker-supervisor-integrity.ts",
    "db:fix-integrity": "tsx scripts/check-worker-supervisor-integrity.ts --fix",
    "i18n:check": "tsx scripts/check-missing-translations.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:worker-supervisor": "tsx tests/integration/worker-supervisor/run-all-tests.ts",
    "test:perf": "playwright test tests/e2e/performance --reporter=list",
    "test:perf:ui": "playwright test tests/e2e/performance --ui",
    "test:perf:report": "playwright test tests/e2e/performance --reporter=html",
    "test:perf:ci": "playwright test tests/e2e/performance --reporter=json --reporter=html"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hookform/resolvers": "^3.9.1",
    "@mui/icons-material": "^6.1.9",
    "@mui/material": "^6.1.9",
    "@mui/x-date-pickers": "^8.21.0",
    "@prisma/client": "^5.22.0",
    "@react-spring/web": "^10.0.3",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-table": "^8.20.5",
    "@types/leaflet": "^1.9.21",
    "@types/leaflet.markercluster": "^1.5.6",
    "@upstash/redis": "^1.35.7",
    "bcryptjs": "^2.4.3",
    "canvas-confetti": "^1.9.4",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "framer-motion": "^11.18.2",
    "html2canvas": "^1.4.1",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "lucide-react": "^0.454.0",
    "maplibre-gl": "^5.14.0",
    "next": "^15.0.3",
    "next-auth": "^5.0.0-beta.25",
    "next-intl": "^3.26.5",
    "next-themes": "^0.4.3",
    "nprogress": "^0.2.0",
    "react": "^19.0.0",
    "react-d3-tree": "^3.6.6",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.53.2",
    "react-hot-toast": "^2.6.0",
    "react-organizational-chart": "^2.2.1",
    "react-swipeable": "^7.0.2",
    "recharts": "^3.5.1",
    "stylis-plugin-rtl": "^2.1.1",
    "web-push": "^3.6.7",
    "web-vitals": "^5.1.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^22.9.3",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/web-push": "^3.6.4",
    "eslint": "^9.14.0",
    "eslint-config-next": "^15.0.3",
    "prisma": "^5.22.0",
    "typescript": "^5.6.3"
  }
}
